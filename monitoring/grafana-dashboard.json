{
  "dashboard": {
    "id": null,
    "title": "Spring Boot Product Service",
    "panels": [
      {
        "type": "graph",
        "title": "JVM Memory Used",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{area=\"heap\"}",
            "legendFormat": "Heap Used"
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "process_cpu_usage",
            "legendFormat": "CPU Usage"
          }
        ],
        "datasource": "Prometheus"
      },
      {
        "type": "graph",
        "title": "HTTP Response Time",
        "targets": [
          {
            "expr": "http_server_requests_seconds_sum{uri=~\"/api/products.*\"} / http_server_requests_seconds_count{uri=~\"/api/products.*\"}",
            "legendFormat": "Avg Response Time"
          }
        ],
        "datasource": "Prometheus"
      }
    ],
    "schemaVersion": 16,
    "version": 1
  }
} 